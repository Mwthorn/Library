public class ISBN13 {

	public static int checkDigit(String a) {
		int total = 0;
		String charAt = "a";
		for (int i = 0; i < a.length(); i++) {
			if (i % 2 == 1)
				total += 3*Integer.parseInt(charAt.replace(charAt.charAt(0), a.charAt(i)));
			else
				total += Integer.parseInt(charAt.replace(charAt.charAt(0), a.charAt(i)));
		}
		
		total = 10 - (total % 10);
		return total;
	}
}
